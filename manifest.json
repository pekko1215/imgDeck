{
    "description": "なんでなかったのか",
    "icons": {
        "32": "ele.png"
    },
    "manifest_version": 2,
    "name": "imgDeck",
    "version": "1",
    "permissions": [
        "tabs",
        "storage",
        "https://*/*"
    ],
    "content_scripts":[
        {
        "matches":["https://tweetdeck.twitter.com/"],
        "js":["jquery.min.js","background.js"]
         }
    ],
    "web_accessible_resources":[
        "*.*"
    ],
    "browser_action": {
        "default_icon": {
            "32": "ele.png"
        }
    }
}
